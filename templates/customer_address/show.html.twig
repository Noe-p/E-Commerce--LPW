{% block body %}
    <h1>CustomerAddress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ customer_address.id }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ customer_address.lastName }}</td>
            </tr>
            <tr>
                <th>FirsName</th>
                <td>{{ customer_address.firsName }}</td>
            </tr>
            <tr>
